

<h2>Listing Artist Involvements</h2>

<table>
  <thead>
    <tr>
      <th>Involvement</th>
      <th>Artist</th>
      <th>Start year</th>
      <th>End year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artist_involvements.each do |artist_involvement| %>
      <tr>
        <td><%= artist_involvement.involvement_id %></td>
        <td><%= artist_involvement.artist_id %></td>
        <td><%= artist_involvement.start_year %></td>
        <td><%= artist_involvement.end_year %></td>
        <td><%= link_to 'Show', artist_involvement %></td>
        <td><%= link_to 'Bewerken', edit_artist_involvement_path(artist_involvement) %></td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artist involvement', new_artist_involvement_path %>
