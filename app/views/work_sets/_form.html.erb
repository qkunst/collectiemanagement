    <%= simple_form_for [@collection, @work_set].compact do |f| %>
      <%= hidden_field_tag name: :collection_id, value: @collection.id if @collection %>
      <%= f.error_notification %>
      <ul>
        <% @work_set.errors.each do |a| %>
          <li><%= a.message%></li>
        <% end %>
      </ul>
      <%= f.association :work_set_type%>
      <%= f.input :identification_number %>
      <%= f.input :comment %>
      <%= f.association :works, as: :hidden%>
      <%= f.submit class: :button%>
    <% end %>
